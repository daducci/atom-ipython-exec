{
  "name": "ipython-exec",
  "main": "./lib/ipython-exec",
  "version": "1.2.0",
  "description": "Send python code to an ipython consoles running in a terminal session",
  "activationCommands": {
    "atom-text-editor": [
      "ipython-exec:open-terminal",
      "ipython-exec:prev-cell",
      "ipython-exec:next-cell"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daducci/atom-ipython-exec.git"
  },
  "keywords": [ "python", "ipython", "terminal", "shell", "run", "execute" ],
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  },
  "dependencies": {
    "node-osascript": "1.0.2"
  }
}
